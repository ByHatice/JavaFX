<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<Pane xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="se.iths.games.tictactoe.Controller">
    <Canvas height="500" width="350" />
   <Label fx:id="headline" alignment="CENTER" contentDisplay="CENTER" layoutX="-2.0" layoutY="1.0" prefHeight="94.0" prefWidth="367.0" text="Tic-Tac-Toe">
      <font>
         <Font size="26.0" />
      </font>
   </Label>
   <Label alignment="TOP_CENTER" layoutX="25.0" layoutY="90.0" prefHeight="20.0" prefWidth="90.0" text="Your Score X:">
      <font>
         <Font size="14.0" />
      </font>
   </Label>
   <Label fx:id="playerScore" layoutX="60.0" layoutY="105.0" prefHeight="25.0" prefWidth="30.0">
      <font>
         <Font size="16.0" />
      </font>
   </Label>
   <Label alignment="TOP_CENTER" layoutX="235.0" layoutY="90.0" prefHeight="20.0" prefWidth="90.0" text="Bot Score O:">
